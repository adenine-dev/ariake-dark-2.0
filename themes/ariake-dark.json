{
  "$schema": "vscode://schemas/color-theme",
  "name": "Ariake Dark 2.0",
  "colors": {
    "editor.lineHighlightBackground": "#65737e30",
    "editor.background": "#1F212A",
    "editorCursor.foreground": "#c0c5ce",
    "editor.selectionBackground": "#343d46",
    "editor.foreground": "#c0c5ce",
    "editorWhitespace.foreground": "#65737e",
    "editorIndentGuide.background": "#3b5364",
    "sideBar.background": "#1c1f26",
    "activityBar.background": "#1c1f26",
    "sideBarSectionHeader.background": "#1c1f26",
    "editorGroupHeader.tabsBackground": "#1c1f26",
    "editorGroup.dropBackground": "#1c1f26",
    "tab.border": "#1c1f26",
    "tab.inactiveForeground": "#65737f",
    "tab.inactiveBackground": "#1c1f26",
    "statusBar.background": "#1c1f26",
    "statusBar.foreground": "#65737f",
    "statusBar.debuggingBackground": "#F03A47",
    "statusBar.debuggingForeground": "#F5FAFF",
    "statusBar.noFolderBackground": "#7e7edd",
    "statusBar.noFolderForeground": "#F5FAFF",
    "titleBar.inactiveBackground": "#1c1f26",
    "titleBar.activeBackground": "#1c1f26",
    "list.activeSelectionBackground": "#343d46",
    "list.inactiveSelectionBackground": "#343d46",
    "list.focusBackground": "#343d46",
    "list.hoverBackground": "#1c1f26",
    "input.background": "#1c1f26",
    "dropdown.background": "#1c1f26",
    "editorWidget.background": "#232830",
    "editorHoverWidget.background": "#232830",
    "editorHoverWidget.border": "#343d46",
    "diffEditor.insertedTextBackground": "#43d08a10",
    "diffEditor.removedTextBackground": "#e052521a",
    "debugToolBar.background": "#1F212A",
    "debugIcon.stepIntoForeground": "#85b1e0",
    "debugIcon.stepOutForeground": "#85b1e0",
    "debugIcon.continueForeground": "#85b1e0",
    "debugIcon.stepOverForeground": "#85b1e0",
    "debugIcon.pauseForeground": "#85b1e0",
    "debugIcon.disconnectForeground": "#F03A47",
    "debugIcon.stopForeground": "#F03A47",
    "editor.stackFrameHighlightBackground": "#343d46",
    "debugIcon.breakpointCurrentStackframeForeground": "#BDC5E8",
    "debugIcon.breakpointForeground": "#F03A47",
    "editorRuler.foreground": "#2D303A",
    "editorWarning.foreground": "#eddfc0",
    "editorError.foreground": "#F03A47",
    "editorInfo.foreground": "#85b1e0",
    "notificationsInfoIcon.foreground": "#9CDCFE",
    "notificationsWarningIcon.foreground": "#eddfc0",
    "notificationsErrorIcon.foreground": "#F03A47",
    "editor.findMatchBackground": "#4f5b66",
    "editor.findMatchHighlightBackground": "#4f5b6699",
    "editor.foldBackground": "#4f5b6620"
  },
  "tokenColors": [
    {
      "scope": [
        "meta.embedded",
        "source.groovy.embedded"
      ],
      "settings": {
        "foreground": "#D4D4D4"
      }
    },
    {
      "scope": [
        "meta.link.inline.markdown",
      ],
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "emphasis",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "strong",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "header",
      "settings": {
        "foreground": "#85b1e0"
      }
    },
    {
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#555C77",
      }
    },
    {
      "scope": "constant.language",
      "settings": {
        "foreground": "#BDC5E8"
      }
    },
    {
      "scope": [
        "constant.numeric"
      ],
      "settings": {
        "foreground": "#9AEFEA"
      }
    },
    {
      "scope": "constant.regexp",
      "settings": {
        "foreground": "#646695"
      }
    },
    {
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#7E7EDD"
      }
    },
    {
      "scope": "entity.name.tag.css",
      "settings": {
        "foreground": "#dda2f6"
      }
    },
    {
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#dda2f6"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.class.mixin.css",
        "entity.other.attribute-name.id.css",
        "entity.other.attribute-name.parent-selector.css",
        "entity.other.attribute-name.pseudo-class.css",
        "entity.other.attribute-name.pseudo-element.css",
        "source.css.less entity.other.attribute-name.id",
        "entity.other.attribute-name.attribute.scss",
        "entity.other.attribute-name.scss"
      ],
      "settings": {
        "foreground": "#b1b2f2"
      }
    },
    {
      "scope": "invalid",
      "settings": {
        "foreground": "#f44747"
      }
    },
    {
      "scope": "markup.underline",
      "settings": {
        "fontStyle": "underline"
      }
    },
    {
      "scope": "markup.bold",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.heading",
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": "markup.italic",
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": "markup.inserted",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "markup.deleted",
      "settings": {
        "foreground": "#D69D85"
      }
    },
    {
      "scope": "markup.changed",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "punctuation.definition.quote.begin.markdown",
      "settings": {
        "foreground": "#9a9fbb"
      }
    },
    {
      "scope": "punctuation.definition.list.begin.markdown",
      "settings": {
        "foreground": "#7D829B"
      }
    },
    {
      "scope": "markup.inline.raw",
      "settings": {
        "foreground": "#F5FAFF"
      }
    },
    {
      "name": "brackets of XML/HTML tags",
      "scope": "punctuation.definition.tag",
      "settings": {
        "foreground": "#7D829B"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#7E7EDD"
      }
    },
    {
      "scope": [
        "meta.preprocessor.string",
      ],
      "settings": {
        "foreground": "#9AEFEA"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "#569cd6"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
        "foreground": "#7E7EDD"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#85b1e0"
      }
    },
    {
      "scope": "string.quoted,string.template",
      "settings": {
        "foreground": "#9AEFEA"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#9AEFEA"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#d16969"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression.begin",
        "punctuation.definition.template-expression.end",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#7D829B"
      }
    },
    {
      "name": "Reset JavaScript string interpolation expression",
      "scope": [
        "meta.template.expression"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "scope": [
        "support.type.vendored.property-name",
        "support.type.property-name",
        "variable.css",
        "variable.scss",
        "variable.other.less",
        "source.coffee.embedded"
      ],
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "keyword",
      "settings": {
        "foreground": "#7E7EDD"
      }
    },
    {
      "scope": [
        "keyword.control",
        "punctuation.definition.directive",
        "keyword.control.at-rule.media"
      ],
      "settings": {
        "foreground": "#7E7EDD"
      }
    },
    {
      "scope": "keyword.operator",
      "settings": {
        "foreground": "#B4B4B4"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.cast",
        "keyword.operator.sizeof",
        "keyword.operator.instanceof",
        "keyword.operator.logical.python"
      ],
      "settings": {
        "foreground": "#7E7EDD"
      }
    },
    {
      "scope": "keyword.other.unit",
      "settings": {
        "foreground": "#9AEFEA"
      }
    },
    {
      "scope": [
        "punctuation.section.embedded.begin.php",
        "punctuation.section.embedded.end.php"
      ],
      "settings": {
        "foreground": "#7D829B"
      }
    },
    {
      "scope": "support.function.git-rebase",
      "settings": {
        "foreground": "#9cdcfe"
      }
    },
    {
      "scope": "constant.sha.git-rebase",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "name": "coloring of the Java import and package identifiers",
      "scope": [
        "storage.modifier.import.java",
        "variable.language.wildcard.java",
        "storage.modifier.package.java"
      ],
      "settings": {
        "foreground": "#d4d4d4"
      }
    },
    {
      "name": "this.self",
      "scope": "variable.language",
      "settings": {
        "foreground": "#7E7EDD"
      }
    },
    {
      "scope": "entity.name",
      "settings": {
        "foreground": "#b9bed5"
      }
    },
    {
      "scope": "keyword.control.directive",
      "settings": {
        "foreground": "#7E7EDD"
      }
    },
    {
      "scope": "variable",
      "settings": {
        "foreground": "#b9bed5"
      }
    },
    {
      "scope": "entity.name.function",
      "settings": {
        "foreground": "#b9bed5"
      }
    },
    {
      "scope": "entity.name.function.operator",
      "settings": {
        "foreground": "#B4B4B4"
      }
    },
    {
      "scope": "entity.name.function.preprocessor",
      "settings": {
        "foreground": "#a571f4"
      }
    },
    {
      "scope": "entity.name.label",
      "settings": {
        "foreground": "#b9bed5"
      }
    },
    {
      "scope": "entity.name.namespace",
      "settings": {
        "foreground": "#b1b2f2"
      }
    },
    {
      "scope": [
        "entity.name.type",
        "entity.name.class",
        "support.class.component.js.jsx"
      ],
      "settings": {
        "foreground": "#A571F4"
      }
    },
    {
      "scope": "entity.name.operator.custom-literal",
      "settings": {
        "foreground": "#C0C5CE"
      }
    },
    {
      "scope": "entity.name.operator.custom-literal.number",
      "settings": {
        "foreground": "#B5CEA8"
      }
    },
    {
      "scope": "entity.name.operator.custom-literal.string",
      "settings": {
        "foreground": "#9AEFEA"
      }
    },
    {
      "scope": [
        "keyword.operator.new",
        "keyword.operator.delete"
      ],
      "settings": {
        "foreground": "#7E7EDD"
      }
    },
    {
      "scope": "variable.other.enummember",
      "settings": {
        "foreground": "#BDC5E8"
      }
    },
    {
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#93defb"
      }
    },
    {
      "scope": "entity.name.function.member",
      "settings": {
        "foreground": "#BDC5E8"
      }
    },
    {
      "scope": "variable.other.property",
      "settings": {
        "foreground": "#BDC5E8"
      }
    }
  ]
}